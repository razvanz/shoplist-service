{
  "name": "shoplist-service",
  "version": "0.0.0",
  "description": "A service providing CRUD functionality for shopping lists through a REST API.",
  "main": "dist/main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "nest build --webpack",
    "prepare": "npm run build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "tc": "tsc --noEmit --incremental false",
    "test": "jest --config jest.config.json",
    "test:unit": "jest --config jest.config.json --testPathPattern='src/.*(test|spec).ts'",
    "test:e2e": "jest --config jest.config.json  --testPathPattern='test/.*(test|spec).ts'",
    "test:coverage": "jest --config jest.config.json --coverage --detectOpenHandles",
    "test:watch": "jest --watch --config jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register jest --config jest.config.json",
    "start": "node dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/razvanz/shoplist-service.git"
  },
  "author": "Razvan Laurus <razvanlaurus@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/razvanz/shoplist-service/issues"
  },
  "homepage": "https://github.com/razvanz/shoplist-service#readme",
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.0",
    "@types/passport": "^1.0.4",
    "class-validator": "^0.12.2",
    "lodash": "^4.17.19",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local-token": "^1.0.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/testing": "^7.3.1",
    "@types/bunyan": "^1.8.6",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.7",
    "@types/express-serve-static-core": "^4.17.8",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.18",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "typemoq": "^2.1.0",
    "typescript": "^3.9.6"
  }
}
